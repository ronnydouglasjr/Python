# Python
print("Codigo do valor comprado: ")
codigo=int(input())
print("Quantidade comprada: ") 
quantidade=int(input())
valor=float

if codigo==1:
	valor= 5.00*quantidade
else:
	if codigo==2:
		valor== 3.50*quantidade	
	else:
		if codigo==3:
			valor= 4.80*quantidade	
		else:
			if codigo== 4:
				valor= 8.90 * quantidade
			else:
				if codigo==5:
					valor= quantidade*7.32 
print("Total a pagar: R$ %.2f" %valor)
