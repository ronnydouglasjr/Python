# Python
print("digite o valor X")
X=float(input())
print("digite o valor Y")
Y=float(input())
if (X>0) and (Y>0):	
	print("Q1")
else:
	if (X<0) and (Y>0):
		print("Q2")
	else:
		if (X>0) and (Y<0):
			print("Q4")
		else:
			if (X<0) and (Y<0):
				print("Q3")
			else:	
				if (X==0) and (Y==0):
					print("Origem")	
				else:
					if (X>0) and (Y==0):
						print("Eixo X")
					else:
						if (X==0) and (Y>0):
							print("Eixo Y")		
			
